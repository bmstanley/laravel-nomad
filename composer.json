{
    "name": "shiftonelabs/laravel-nomad",
    "description": "Add extra functionality to Laravel migrations.",
    "keywords": ["laravel", "lumen", "migration", "custom"],
    "homepage": "https://github.com/shiftonelabs/laravel-nomad",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Carlo-Hickman",
            "email": "patrick@shiftonelabs.com"
        }
    ],
    "support": {
        "issues": "https://github.com/shiftonelabs/laravel-nomad/issues",
        "source": "https://github.com/shiftonelabs/laravel-nomad"
    },
    "require": {
        "php": ">=7.0.0",
        "illuminate/database": "5.5.*"
    },
    "require-dev": {
        "doctrine/dbal": "^2.5",
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~0.9",
        "fzaninotto/faker": "~1.5",
        "squizlabs/php_codesniffer": "^2.6",
        "orchestra/testbench": "3.5.*",
        "orchestra/database": "3.5.*"
    },
    "autoload": {
        "psr-4": {
            "ShiftOneLabs\\LaravelNomad\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ShiftOneLabs\\LaravelNomad\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ShiftOneLabs\\LaravelNomad\\LaravelNomadServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
